    "build:scss": "sass src/scss/goc-admin-vue-build.scss | node ./build/banner.js > dist/goc-admin-vue.css && cleancss -o dist/goc-admin-vue.min.css dist/goc-admin-vue.css && sass src/scss/goc-admin-vue-rtl.scss > dist/goc-admin-vue-rtl.css &&cleancss -o dist/goc-admin-vue-rtl.min.css dist/goc-admin-vue-rtl.css",
    "build:scss:watch": "sass src/scss/goc-admin-vue-build.scss --watch && sass src/scss/goc-admin-vue-rtl.scss --watch",
    "prepare": "husky install"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/gocteam"
  },
  "author": {
    "name": "gocteam",
